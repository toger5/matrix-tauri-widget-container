{
  "build": {
    "withGlobalTauri": true,
    "devPath": "../src/"
  },
  "package": {
    "productName": "tauri-widget-container",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "io.element.tauri-widget-container",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null,
      "dangerousRemoteDomainIpcAccess": [
        {
          "domain": "call.element.dev",
          "windows": ["widget_window"],
          "enableTauriAPI": true
        },
        {
          "domain": "call.element.io",
          "windows": ["widget_window"],
          "enableTauriAPI": true
        },
        {
          "domain": "pr1348--element-call.netlify.app",
          "windows": ["widget_window"],
          "enableTauriAPI": true
        },
        {
          "domain": "pr1474--element-call.netlify.app",
          "windows": ["widget_window"],
          "enableTauriAPI": true
        }
      ]
    },
    "windows": []
  }
}
